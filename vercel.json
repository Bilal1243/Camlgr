{
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "https://old-sitemap.vercel.app/sitemap.xml",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "https://camlgr.in/$1",
      "permanent": true
    }
  ]
}
